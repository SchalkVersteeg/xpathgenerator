			<!-- ad hoc configs -->
			<defineLocationType>
				<location userLocationType="Store"		value="S">store</location>
				<location userLocationType="District"	value="D">store</location>
				<location userLocationType="Region"		value="R">store</location>
				<location userLocationType="Warehouse"	value="S">whse</location>
				<!-- note: "all" type is hardcoded to show all, not matter what; not enough time to fix -->
			</defineLocationType>	
			
			<verdeSecurityAssoc>
				<verdeAssoc managerFlag="1">
					<groups>mtr_basicUser</groups>
					<displayFilter>NSTORE = {0}</displayFilter>
					<fromFilter>NSTORE = {0}</fromFilter>
				</verdeAssoc>
				<verdeAssoc managerFlag="2">
					<groups>mtr_districtUser</groups>
					<displayFilter>DISTRICT = {1} or ST_NAME like 'Warehouse%'</displayFilter><!-- or ST_NAME like 'Corporate%' -->
					<fromFilter>(DISTRICT = {1} and ST_NAME like 'Store%') or ST_NAME like 'Warehouse%'</fromFilter><!-- or ST_NAME like 'Corporate%'-->
				</verdeAssoc>
				<verdeAssoc managerFlag="3">
					<groups>mtr_regionalUser</groups>
					<displayFilter>REGION = {2} or ST_NAME like 'Warehouse%'</displayFilter><!-- or ST_NAME like 'Corporate%' -->
					<fromFilter>(REGION = {2} and ST_NAME like 'Store%') or ST_NAME like 'Warehouse%'</fromFilter><!-- or ST_NAME like 'Corporate%' -->
				</verdeAssoc>
				<verdeAssoc managerFlag="4">
					<groups>mtr_basicUser</groups>				
					<displayFilter>NSTORE = {0}</displayFilter>
					<fromFilter>NSTORE = {0}</fromFilter>
				</verdeAssoc>
				<!-- REM 0-4 are stores w/o access to AD or user role DB; 5 uses user role DB -->
				<verdeAssoc managerFlag="5">
					<groups>mtr_corpUser mtr_corpPowerUser mtr_whseUser mtr_whsePowerUser mtr_administrator</groups>
					<displayFilter />	
					<fromFilter>ST_NAME like 'Store%' or ST_NAME like 'Warehouse%'</fromFilter><!-- or ST_NAME like 'Corporate%'-->
				</verdeAssoc>
			</verdeSecurityAssoc>
			
			<mtrStatus>
				<status value="T">All</status>
				<status value="D">Discrepancy</status>
				<status value="A">Accepted</status>
				<status value="X">Declined</status>
				<status value="I">Pending</status>
			</mtrStatus>

			<orderType>
			<!-- coincides with .aspx page names; e.g. ItemRecv.aspx, RetailRecv.aspx, etc.; yeah, it's dumb; quick db/page definition -->
				<status value="I">Item</status>
				<status value="D">Retail</status>
			</orderType>

