					</mtrTypeCombinations>
					<orderTypes>D I</orderTypes>
					<receivingType>auto</receivingType>
					<whseFlags>sendout whse</whseFlags>
				</send>
				<send>
					<senders>store</senders>
					<receivers>whse</receivers>
					<sendGroups>mtr_administrator mtr_districtUser mtr_regionalUser</sendGroups>
					<receiveGroups>mtr_administrator mtr_whsePowerUser</receiveGroups>
					<itemStatuses>active</itemStatuses>
					<mtrTypes>fixtures merchandise racks supplies buildMaterial</mtrTypes>
					<mtrTypeCombinations>
						<groupMtrTypeCombo>fixtures racks</groupMtrTypeCombo>
					</mtrTypeCombinations>
					<orderTypes>I</orderTypes>
					<receivingType>manual</receivingType>
					<whseFlags>whse</whseFlags>
				</send>
				<send>
					<senders>store</senders>
					<receivers>whse</receivers>
					<sendGroups>mtr_administrator mtr_whsePowerUser</sendGroups>
					<receiveGroups>mtr_administrator mtr_whsePowerUser</receiveGroups>
					<itemStatuses>active</itemStatuses>
					<mtrTypes>fixtures merchandise racks supplies buildMaterial</mtrTypes>
					<mtrTypeCombinations>
						<groupMtrTypeCombo>fixtures racks</groupMtrTypeCombo>
					</mtrTypeCombinations>
					<orderTypes>D</orderTypes>
					<receivingType>manual</receivingType>
					<whseFlags>sendout</whseFlags>
				</send>
			</sending>

			<receipt>
				<print>
					<direction>incoming</direction>
					<quantity>1</quantity>
					<message>One page has been automatically printed.  Please keep this document for your records.</message>
				</print>
				<print>
					<direction>outgoing</direction>
					<quantity>2</quantity>
					<message>Two pages have been automatically printed.  Please send one with the shipment, and keep the other for your records.</message>
				</print>
			</receipt>

			<pageAccess>
